<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shivam.filelibrarycheck">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/tick_green"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppThemeNO">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".markAttendance"
            android:label="Mark Attendance"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainActivity"
            android:label="Select Student Data" />
        <activity android:name=".Piechart" />
        <activity android:name=".Metadata" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivityin" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CreateData">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shivam.filelibrarycheck.MainActivity" />
        </activity>
        <activity android:name=".AttendanceList" />
        <activity android:name=".ViewAttendance">
        </activity>
    </application>

</manifest>